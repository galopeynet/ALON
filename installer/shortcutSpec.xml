<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="ALON" location="startMenu"/>
	<shortcut
		name="ALON Server"
		target="$INSTALL_PATH\ALON.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start ALON server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="ALON Wallet"
		target="http://localhost:8003/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch ALON wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>